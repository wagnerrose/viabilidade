<div class="row">
  <div class="col-md-1"><center><%= texto %></center></div>        
  <div class="col-md-1"><%= f.estrutura %></div> 
  <div class="col-md-3"><%= f.descricao %></div>
  <div align="center" class="col-md-1"><%= f.prazo %></div> 
  <div align="center" class="col-md-1"><%= f.capex %></div> 
  <div align="center"class="col-md-1"><%= f.opex_instalacao %></div> 
  <div align="center" class="col-md-1"><%= f.opex_mensal %></div> 
  <div align="center" class="col-md-1"><%= f.resultado_vt %></div>
  <div align="center" class="col-md-1">
    <div class="col-md-6">
      <%= link_to image_tag('ExcluirPropriedade-16.png', title:'Excluir'),
        resposta_path(f[:id]),
        :method => 'delete',
        :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) } %>
    </div><!-- botao excluir -->
    <div class="col-md-6">
      <%= link_to image_tag('EditarPropriedade-16.png', title:'Edita'),
        edit_resposta_path(f[:id]),
        :method => 'edit' %>
    </div><!-- botao edita -->
  </div><!-- botoes -->
</div><!-- row -->
