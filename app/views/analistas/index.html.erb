<%- model_class = Analista -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:nome) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th><%= model_class.human_attribute_name(:id_regional) %></th>
      <th><%= model_class.human_attribute_name(:fone_contato) %></th>
      <th><%= model_class.human_attribute_name(:funcao) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @analistas.each do |analista| %>
      <tr>
        <td><%= link_to analista.id, analista_path(analista) %></td>
        <td><%= analista.nome %></td>
        <td><%= analista.email %></td>
        <td><%= analista.id_regional %></td>
        <td><%= analista.fone_contato %></td>
        <td><%= analista.funcao %></td>
        <td><%=l analista.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      analista_path(analista), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_analista_path(analista), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      analista_path(analista),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_analista_path,
            :class => 'btn btn-primary' %>
