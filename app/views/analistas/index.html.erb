<p id="notice"><%= notice %></p>

<h1>Analistas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Email</th>
      <th>Id regional</th>
      <th>Fone Contato</th>
      <th>Função</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analistas.each do |analista| %>
      <tr>
        <td><%= analista.nome %></td>
        <td><%= analista.email %></td>
        <td><%= analista.id_regional %></td>
        <td><%= analista.fone_contato %></td>
        <td><%= analista.funcao %></td>
        <td><%= link_to 'Mostrar', analista %></td>
        <td><%= link_to 'Editar', edit_analista_path(analista) %></td>
        <td><%= link_to 'Apagar', analista, method: :delete, data: { confirm: 'Você tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Cadastro de novo Analista', new_analista_path %>
