<p id="notice"><%= notice %></p>

<h1>Estacoes</h1>

<table>
  <thead>
    <tr>
      <th>Pop</th>
      <th>Nome</th>
      <th>Endereco</th>
      <th>Cidade</th>
      <th>Uf</th>
      <th>Tipo</th>
      <th>Cedente</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estacoes.each do |estacao| %>
      <tr>
        <td><%= estacao.pop %></td>
        <td><%= estacao.nome %></td>
        <td><%= estacao.endereco %></td>
        <td><%= estacao.cidade %></td>
        <td><%= estacao.uf %></td>
        <td><%= estacao.tipo %></td>
        <td><%= estacao.cedente %></td>
        <td><%= estacao.latitude %></td>
        <td><%= estacao.longitude %></td>
        <td><%= link_to 'Show', estacao %></td>
        <td><%= link_to 'Edit', edit_estacao_path(estacao) %></td>
        <td><%= link_to 'Destroy', estacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estacao', new_estacao_path %>
