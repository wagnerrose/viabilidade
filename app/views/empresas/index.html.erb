<%- model_class = Empresa -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <!--<th><%= model_class.human_attribute_name(:id) %></th>-->
      <th><%= model_class.human_attribute_name(:razao) %></th>
      <th><%= model_class.human_attribute_name(:nome_fantasia) %></th>
      <th><%= model_class.human_attribute_name(:cnpj) %></th>
      <th><%= model_class.human_attribute_name(:sap) %></th>
      <th><%= model_class.human_attribute_name(:endereco) %></th>
      <th><%= model_class.human_attribute_name(:cidade) %></th>
      <th><%= model_class.human_attribute_name(:cep) %></th>
      <th><%= model_class.human_attribute_name(:uf) %></th>
      <th><%= model_class.human_attribute_name(:nome_contato) %></th>
      <th><%= model_class.human_attribute_name(:fixo) %></th>
      <th><%= model_class.human_attribute_name(:celular) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <!--<th><%= model_class.human_attribute_name(:created_at) %></th>-->
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <!--<td><%= link_to empresa.id, empresa_path(empresa) %></td>-->
        <td><%= empresa.razao %></td>
        <td><%= empresa.nome_fantasia %></td>
        <td><%= empresa.cnpj %></td>
        <td><%= empresa.sap %></td>
        <td><%= empresa.endereco %></td>
        <td><%= empresa.cidade %></td>
        <td><%= empresa.cep %></td>
        <td><%= empresa.uf %></td>
        <td><%= empresa.nome_contato %></td>
        <td><%= empresa.fixo %></td>
        <td><%= empresa.celular %></td>
        <td><%= empresa.email %></td>
        <!--<td><%=l empresa.created_at %></td>-->
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      empresa_path(empresa), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_empresa_path(empresa), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      empresa_path(empresa),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_empresa_path,
            :class => 'btn btn-primary' %>
