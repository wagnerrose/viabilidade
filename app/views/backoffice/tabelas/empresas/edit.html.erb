<!-- carregamento inteligente dos CSS e JAVASCRIPTS  -->
<% content_for :head do %>
  <%= stylesheet_link_tag params[:controller] %>
  <%= javascript_include_tag params[:controller] %>
<% end %>

<%- model_class = Empresa -%>

<!-- Page Content -->
<div id="page-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">

        <!-- codigo da pagina -->
        <div class="list-group">
          <h1  class="list-group-item-heading list-group-item-info">
            <center><%=t '.title', :default => [:'helpers.titles.edit', 'Edit %{model}'],
              :model => model_class.model_name.human.titleize %></center>
          </h1>
        </div>
        <%= render :partial => 'form' %>
        <!--/ codigo da pagina -->

      </div>
      <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</div>

<!-- / #page-wrapper -->
<!-- / #wrapper <-->
