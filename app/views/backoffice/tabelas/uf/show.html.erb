

<%- model_class = Uf -%>
<!-- Page Content -->
<div id="page-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">

        <!-- codigo da pagina -->
        <div class="box-show">
          <div class="list-group">
            <h1 class="list-group-item-heading list-group-item-info" style="margin: 5px;">
                  <center><%=t '.title', :default => model_class.model_name.human.titleize.upcase %></center>
            </h1>
          </div> <!-- list-group-->

          <dl class="dl-horizontal">
            <dt><strong><%= model_class.human_attribute_name(:sigla) %>:</strong></dt>
            <dd><%= @uf.sigla %></dd>
            <dt><strong><%= model_class.human_attribute_name(:nome) %>:</strong></dt>
            <dd><%= @uf.nome %></dd>
          </dl>
        </div>
        <div class="box-show">
          <div class="dl-horizontal">
            <center>
            <%= link_to t('.back', :default => t("helpers.links.back")),
                          backoffice_tabelas_uf_index_path, :class => 'btn botao btn-sm'  %>
            <%= link_to t('.edit', :default => t("helpers.links.edit")),
                          backoffice_tabelas_uf_path(@uf),
                          :method => 'edit',
                          :class => 'btn botao btn-sm' %>
            <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                          backoffice_tabelas_uf_path(@uf),
                          :method => 'delete',
                          :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                          :class => 'btn botaoApaga btn-sm' %>
            </center>
          </div>
        </div>

        <!--/ codigo da pagina -->

      </div>
      <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</div>

<!-- / #page-wrapper -->
<!-- / #wrapper <-->
